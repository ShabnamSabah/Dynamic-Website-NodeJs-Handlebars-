                                    <div class="roadmap-info">
                                        <div class="timestamp completed">
                                            <span class="date">{{date1}}</span>
                                        </div>
                                        <div class="status completed">
                                            <span>{{date1_details}}</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="swiper-slide">
