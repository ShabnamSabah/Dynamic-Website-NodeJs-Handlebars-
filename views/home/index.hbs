                                                <span class="icon"></span>
                                                {{this.dataValues.question}}
                                            </a>

                                    </div>
                                    <div id="collapse{{this.dataValues.id}}" class="collapse show"
                                        aria-labelledby="heading{{this.dataValues.id}}"
                                        data-parent="#{{@index}}-accordion">

                                        <div class="card-body">
                                            {{this.dataValues.answer}}
                                        </div>

                                    </div>

                                </div>
                                {{/each}}


                            </div>

                        </div>
                        {{else}}

                        <div class="tab-pane fade" id="{{this.dataValues.qtype}}" role="tabpanel"
                            aria-labelledby="{{@index}}-tab">


                            <div id="{{this.dataValues.qtype}}-accordion" class="collapse-icon accordion-icon-rotate">
                                {{#each this.question_answers}}
                                <div class="card animated" data-animation="fadeInUpShorter" data-animation-delay="0.1s">


                                    <div class="card-header" id="heading{{this.dataValues.id}}">
                                        <h5 class="mb-0">

                                            <a class="btn-link" data-toggle="collapse"
                                                data-target="#collapse{{this.dataValues.id}}" aria-expanded="true"
                                                aria-controls="collapse{{this.dataValues.id}}">
                                                <span class="icon"></span>
                                                {{this.dataValues.question}}
                                            </a>

                                    </div>
                                    <div id="collapse{{this.dataValues.id}}" class="collapse show"
                                        aria-labelledby="heading{{this.dataValues.id}}"
                                        data-parent="#{{this.dataValues.qtype}}-accordion">

                                        <div class="card-body">
                                            {{this.dataValues.answer}}
                                        </div>

                                    </div>

                                </div>
                                {{/each}}


                            </div>

                        </div>




                        {{/calculation}}
                        {{/each}}

                    </div>



                </div>
            </div>
        </div>
    </div>
</section>
