<div class="content">
        <div class="container-fluid">
          <div class="row">
            <div class="col-md-12">
              <div class="card">
                <div class="card-header card-header-primary card-header-icon">
                  <div class="card-icon">
                    <i class="material-icons">assignment</i>
                  </div>
                  <h4 class="card-title">List of Question Answer</h4>
                </div>
                <div class="card-body">
                  
                  <div class="table-responsive">
                    <table  class="table table-striped">
                      <thead>
                        <tr>
                        
                          <th>Question </th>
                          <th> Answer</th>
                          
                          <th class="text-right">Actions</th>
                        </tr>
                      </thead>
                
                      <tbody>
                        {{#each question_answer}}
                        {{#dataValues}}
                        <tr>
                         
                          <td>{{question}}</td>
                          <td>{{answer}}</td>
                          <td class="text-right" style='white-space: nowrap'>
                            <a href="/admin/edit-question-answer/{{id}}" class="btn btn-link btn-info btn-just-icon like"><i class="material-icons">edit</i></a>
                            <a href="/admin/delete-question-answer/{{id}}" class="btn btn-link btn-danger btn-just-icon remove"><i class="material-icons">close</i></a>
                          </td>
                        </tr>
                        {{/dataValues}}
                        {{/each}}
                      </tbody>
                    </table>
                    </div>
                  </div>
               
                <!-- end content-->
              </div>
              <!--  end card  -->
            </div>
            <!-- end col-md-12 -->
          </div>
          <!-- end row -->
        </div>
      </div>